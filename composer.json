{
  "name": "digital-drifter/laravel-cli-seeder",
  "authors": [
    {
      "name": "Digital Drifter",
      "email": "whydoesmycodework@gmail.com"
    }
  ],
  "type": "library",
  "description": "Generate schema based test data from the cli.",
  "keywords": [
    "laravel",
    "seed",
    "faker",
    "database",
    "generator"
  ],
  "require-dev": {
    "doctrine/dbal": "2.7.*",
    "fzaninotto/faker": "1.7.*",
    "illuminate/config": "5.6.*",
    "illuminate/database": "5.6.*",
    "illuminate/support": "5.6.*"
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "DigitalDrifter\\LaravelCliSeeder\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "DigitalDrifter\\LaravelCliSeeder\\ServiceProvider"
      ]
    }
  }
}
